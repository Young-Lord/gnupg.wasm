{
  KBNODE node;
  int status;
  int any = 0;

  for (node=keyblock; node; node = node->next)
    {
      if (node->pkt->pkttype == PKT_USER_ID)
        {
          PKT_user_id *uid = node->pkt->pkt.user_id;
          if (node->flag & 4)
            status = 5;
          else if (node->flag & 2)
            status = 4;
          else if (node->flag & 1)
            status = 2;
          else
            status = 0;

          if (status)
            {
              update_validity (keyblock->pkt->pkt.public_key,
          uid, depth, status);

       mark_keyblock_seen(stored,keyblock);

              any = 1;
            }
        }
    }

  if (any)
    do_sync ();
}





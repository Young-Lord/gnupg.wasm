static int
read_trust_record (PKT_public_key *pk, TRUSTREC *rec)
{
  int rc;

  init_trustdb();
  rc = tdbio_search_trust_bypk (pk, rec);
  if (rc == -1)
    return -1;
  if (rc)
    {
      g10_log_error ("trustdb: searching trust record failed: %s\n",
                 g10_errstr (rc));
      return rc;
    }

  if (rec->rectype != 12)
    {
      g10_log_error ("trustdb: record %lu is not a trust record\n",
                 rec->recnum);
      return 33;
    }

  return 0;
}






static void
update_validity (PKT_public_key *pk, PKT_user_id *uid,
                 int depth, int validity)
{
  TRUSTREC trec, vrec;
  int rc;
  ulong recno;

  namehash_from_uid(uid);

  rc = read_trust_record (pk, &trec);
  if (rc && rc != -1)
    {
      tdbio_invalid ();
      return;
    }
  if (rc == -1)
    {
      size_t dummy;

      rc = 0;
      memset (&trec, 0, sizeof trec);
      trec.recnum = tdbio_new_recnum ();
      trec.rectype = 12;
      fingerprint_from_pk (pk, trec.r.trust.fingerprint, &dummy);
      trec.r.trust.ownertrust = 0;
      }


  recno = trec.r.trust.validlist;
  while (recno)
    {
      read_record (recno, &vrec, 13);
      if ( !memcmp (vrec.r.valid.namehash, uid->namehash, 20) )
        break;
      recno = vrec.r.valid.next;
    }

  if (!recno)
    {
      memset (&vrec, 0, sizeof vrec);
      vrec.recnum = tdbio_new_recnum ();
      vrec.rectype = 13;
      memcpy (vrec.r.valid.namehash, uid->namehash, 20);
      vrec.r.valid.next = trec.r.trust.validlist;
      trec.r.trust.validlist = vrec.recnum;
    }
  vrec.r.valid.validity = validity;
  vrec.r.valid.full_count = uid->help_full_count;
  vrec.r.valid.marginal_count = uid->help_marginal_count;
  write_record (&vrec);
  trec.r.trust.depth = depth;
  write_record (&trec);
}







int
cache_disabled_value(PKT_public_key *pk)
{
  int rc;
  TRUSTREC trec;
  int disabled=0;

  if(pk->is_disabled)
    return (pk->is_disabled==2);

  init_trustdb();
  if (trustdb_args.no_trustdb)
    return 0;

  rc = read_trust_record (pk, &trec);
  if (rc && rc != -1)
    {
      tdbio_invalid ();
      goto leave;
    }
  if (rc == -1)
    goto leave;

  if(trec.r.trust.ownertrust & 128)
    disabled=1;



  if(disabled)
    pk->is_disabled=2;
  else
    pk->is_disabled=1;

 leave:
   return disabled;
}

void
check_trustdb_stale(void)
{
  static int did_nextcheck=0;

  init_trustdb ();
  if (trustdb_args.no_trustdb)
    return;

  if (!did_nextcheck
      && (opt.trust_model==TM_PGP || opt.trust_model==TM_CLASSIC))
    {
      ulong scheduled;

      did_nextcheck = 1;
      scheduled = tdbio_read_nextcheck ();
      if ((scheduled && scheduled <= make_timestamp ())
   || pending_check_trustdb)
        {
          if (opt.no_auto_check_trustdb)
            {
              pending_check_trustdb = 1;
              g10_log_info (gettext ("please do a --check-trustdb\n"));
            }
          else
            {
              g10_log_info (gettext ("checking the trustdb\n"));
              validate_keys (0);
            }
        }
    }
}






unsigned int
get_validity (PKT_public_key *pk, PKT_user_id *uid)
{
  TRUSTREC trec, vrec;
  int rc;
  ulong recno;
  unsigned int validity;
  u32 kid[2];
  PKT_public_key *main_pk;

  if(uid)
    namehash_from_uid(uid);

  init_trustdb ();





  if (trustdb_args.no_trustdb && opt.trust_model == TM_ALWAYS)
    return 0;

  check_trustdb_stale();

  keyid_from_pk (pk, kid);
  if (pk->main_keyid[0] != kid[0] || pk->main_keyid[1] != kid[1])
    {
      main_pk = xmalloc_clear (sizeof *main_pk);
      rc = get_pubkey (main_pk, pk->main_keyid);
      if (rc)
        {
   char *tempkeystr=xstrdup(keystr(pk->main_keyid));
          g10_log_error ("error getting main key %s of subkey %s: %s\n",
                     tempkeystr, keystr(kid), g10_errstr(rc));
   xfree(tempkeystr);
          validity = 0;
          goto leave;
 }
    }
  else
    main_pk = pk;

  if(opt.trust_model==TM_DIRECT)
    {


      validity=get_ownertrust(main_pk);
      goto leave;
    }

  rc = read_trust_record (main_pk, &trec);
  if (rc && rc != -1)
    {
      tdbio_invalid ();
      return 0;
    }
  if (rc == -1)
    {
      validity = 0;
      goto leave;
    }


  recno = trec.r.trust.validlist;
  validity = 0;
  while (recno)
    {
      read_record (recno, &vrec, 13);

      if(uid)
 {




   if(memcmp(vrec.r.valid.namehash,uid->namehash,20)==0)
     {
       validity=(vrec.r.valid.validity & 15);
       break;
     }
 }
      else
 {


   if ( validity < (vrec.r.valid.validity & 15) )
     validity = (vrec.r.valid.validity & 15);
 }

      recno = vrec.r.valid.next;
    }

  if ( (trec.r.trust.ownertrust & 128) )
    {
      validity |= 128;
      pk->is_disabled=2;
    }
  else
    pk->is_disabled=1;

 leave:

  if (main_pk->is_revoked)
    validity |= 32;
  if (main_pk != pk && pk->is_revoked)
    validity |= 64;


  if (main_pk->has_expired || pk->has_expired)
    validity = (validity & ~15) | 1;

  if (pending_check_trustdb)
    validity |= 256;

  if (main_pk != pk)
    free_public_key (main_pk);
  return validity;
}


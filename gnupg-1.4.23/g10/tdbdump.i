    ulong recnum;
    int i;
    byte *p;

    init_trustdb();
    printf(gettext ("# List of assigned trustvalues, created %s\n" "# (Use \"gpg --import-ownertrust\" to restore them)\n"),

    asctimestamp( make_timestamp() ) );
    for(recnum=0; !tdbio_read_record( recnum, &rec, 0); recnum++ ) {
 if( rec.rectype == 12 ) {
     if( !rec.r.trust.ownertrust )
  continue;
     p = rec.r.trust.fingerprint;
     for(i=0; i < 20; i++, p++ )
  printf("%02X", *p );
     printf(":%u:\n", (unsigned int)rec.r.trust.ownertrust );
 }
    }
}


void
import_ownertrust( const char *fname )
{
    FILE *fp;
    int is_stdin=0;
    char line[256];
    char *p;
    size_t n, fprlen;
    unsigned int otrust;
    byte fpr[20];
    int any = 0;
    int rc;

    init_trustdb();
    if( iobuf_is_pipe_filename (fname) ) {
 fp = (stdin);
 fname = "[stdin]";
 is_stdin = 1;
    }
    else if( !(fp = fopen( fname, "r" )) ) {
 g10_log_error ( gettext ("can't open `%s': %s\n"), fname, strerror((*__errno_location())) );
 return;
    }

    if (is_secured_file (fileno (fp)))
      {
        fclose (fp);
        (*__errno_location()) = (63);
 g10_log_error (gettext ("can't open `%s': %s\n"), fname, strerror((*__errno_location())) );
 return;
      }

    while( fgets( line, (sizeof(line)/sizeof((line)[0]))-1, fp ) ) {
 TRUSTREC rec;

 if( !*line || *line == '#' )
     continue;
 n = strlen(line);
 if( line[n-1] != '\n' ) {
     g10_log_error (gettext ("error in `%s': %s\n"), fname, gettext ("line too long") );

     break;
 }
 for(p = line; *p && *p != ':' ; p++ )
     if( !((*(p) >= '0' && *(p) <= '9') || (*(p) >= 'A' && *(p) <= 'F') || (*(p) >= 'a' && *(p) <= 'f')) )
  break;
 if( *p != ':' ) {
     g10_log_error (gettext ("error in `%s': %s\n"), fname, gettext ("colon missing") );
     continue;
 }
 fprlen = p - line;
 if( fprlen != 32 && fprlen != 40 ) {
     g10_log_error (gettext ("error in `%s': %s\n"),
                       fname, gettext ("invalid fingerprint") );
     continue;
 }
 if( sscanf(p, ":%u:", &otrust ) != 1 ) {
     g10_log_error (gettext ("error in `%s': %s\n"),
                       fname, gettext ("ownertrust value missing"));
     continue;
 }
 if( !otrust )
     continue;

 for(p=line, fprlen=0; fprlen < 20 && *p != ':'; p += 2 )
     fpr[fprlen++] = ( (p[0]) >= '0' && (p[0]) <= '9' ? ((p[0])-'0') : (p[0]) >= 'A' && (p[0]) <= 'F' ? ((p[0])-'A'+10) : ((p[0])-'a'+10)) * 16 + ( (p[1]) >= '0' && (p[1]) <= '9' ? ((p[1])-'0') : (p[1]) >= 'A' && (p[1]) <= 'F' ? ((p[1])-'A'+10) : ((p[1])-'a'+10));
 while (fprlen < 20)
     fpr[fprlen++] = 0;

 rc = tdbio_search_trust_byfpr (fpr, &rec);
 if( !rc ) {
     if (rec.r.trust.ownertrust != otrust)
              {
                if( rec.r.trust.ownertrust )
                  g10_log_info("changing ownertrust from %u to %u\n",
                           rec.r.trust.ownertrust, otrust );
                else
                  g10_log_info("setting ownertrust to %u\n", otrust );
                rec.r.trust.ownertrust = otrust;
                write_record (&rec );
                any = 1;
              }
 }
 else if( rc == -1 ) {
            g10_log_info("inserting ownertrust of %u\n", otrust );
            memset (&rec, 0, sizeof rec);
            rec.recnum = tdbio_new_recnum ();
            rec.rectype = 12;
            memcpy (rec.r.trust.fingerprint, fpr, 20);
            rec.r.trust.ownertrust = otrust;
            write_record (&rec );
            any = 1;
 }
 else
     g10_log_error (gettext ("error finding trust record in `%s': %s\n"),
                       fname, g10_errstr(rc));
    }
    if( ferror(fp) )
 g10_log_error ( gettext ("read error in `%s': %s\n"), fname, strerror((*__errno_location())) );
    if( !is_stdin )
 fclose(fp);

    if (any)
      {
        revalidation_mark ();
        rc = tdbio_sync ();
        if (rc)
          g10_log_error (gettext ("trustdb: sync failed: %s\n"), g10_errstr(rc) );
      }

}
